{
    "name": "ottosmops/md5sum",
    "type": "library",
    "description": "create and verifie md5sum files",
    "keywords": [
        "ottosmops",
        "md5sum",
        "checksum", 
        "laravel",
        "anton"
    ],
    "homepage": "https://github.com/ottosmops/md5sum",
    "license": "MIT",
    "authors": [
        {
            "name": "ak",
            "email": "kraenzle@k-r.ch",
            "homepage": "https://wwwk-r.ch"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ottosmops\\Md5sum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ottosmops\\Md5sum\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
